{
  "name": "oniyi-http-client",
  "version": "1.2.0",
  "description": "Adding a plugin interface to \"request\" that allows modifications of request parameters and response data",
  "homepage": "https://github.com/benkroeger/oniyi-http-client#readme",
  "author": "Benjamin Kroeger <benjamin.kroeger@gmail.com>",
  "files": [
    "lib/",
    "plugins/"
  ],
  "main": "lib/index.js",
  "keywords": [
    "request",
    "plugin",
    "http"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/benkroeger/oniyi-http-client.git"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "eslint": "^4.9.0",
    "eslint-config-oniyi": "^5.0.2",
    "eslint-plugin-ava": "^4.2.1",
    "jsdoc-to-markdown": "3.0.0",
    "prettier-eslint": "^8.2.1",
    "prettier-eslint-cli": "^4.4.0",
    "nyc": "11.2.1"
  },
  "scripts": {
    "format": "prettier-eslint --write \"lib/**/*.js\" \"plugins/**/*.js\" \"test/**/*.js\"",
    "prelint": "npm run format",
    "lint": "eslint --ignore-path .gitignore .",
    "test": "ava --verbose",
    "test:watch": "npm test -- --watch",
    "coverage": "nyc npm test && nyc report --reporter=html",
    "docs": "jsdoc2md lib/**/*.js > API.md"
  },
  "license": "MIT",
  "dependencies": {
    "async": "^2.5.0",
    "debug": "^3.1.0",
    "lodash": "^4.17.4",
    "loopback-phase": "^3.1.0",
    "oniyi-logger": "^1.0.0",
    "request": "^2.83.0",
    "tough-cookie": "^2.3.3"
  },
  "bugs": {
    "url": "https://github.com/benkroeger/oniyi-http-client/issues"
  },
  "directories": {
    "test": "test"
  }
}
